<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema id="com.github.wwmm.easyeffects.rnnoise">
        <key name="bypass" type="b">
            <default>false</default>
        </key>
        <key name="input-gain" type="d">
            <range min="-36" max="36" />
            <default>0</default>
        </key>
        <key name="output-gain" type="d">
            <range min="-36" max="36" />
            <default>0</default>
        </key>
        <key name="model-name" type="s">
            <default>""</default>
        </key>
        <key name="enable-vad" type="b">
            <default>false</default>
        </key>
        <key name="vad-thres" type="d">
            <range min="0" max="100" />
            <default>50</default>
        </key>
        <key name="wet" type="d">
            <range min="-100" max="20" />
            <default>0.0</default>
        </key>
        <key name="release" type="d">
            <range min="0" max="20000" />
            <default>20.0</default>
        </key>
    </schema>
</schemalist>

        </key>
        <key name="output-gain" type="d">
            <range min="-36" max="36" />
            <default>0</default>
        </key>
        <key name="amount" type="d">
            <range min="-100" max="36" />
            <default>0</default>
        </key>
        <key name="harmonics" type="d">
            <range min="0.1" max="10" />
            <default>8.5</default>
        </key>
        <key name="scope" type="d">
            <range min="10" max="250" />
            <default>100</default>
        </key>
        <key name="floor" type="d">
            <range min="10" max="120" />
            <default>20</default>
        </key>
        <key name="blend" type="d">
            <range min="-10" max="10" />
            <default>0</default>
        </key>
        <key name="floor-active" type="b">
            <default>false</default>
        </key>
        <key name="listen" type="b">
            <default>false</default>
        </key>
    </schema>
</schemalist>